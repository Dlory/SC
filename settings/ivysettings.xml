<ivysettings>
	<settings defaultResolver="main"/>
	<properties file="./settings.properties" />
	<include file="ivysettings-nexus.xml" />
	<include file="ivysettings-main-chain.xml" />

	<!-- nexus的连接 -->
	<ivy:configure url="./ivysettings.xml">
		<credentials host="${remote-host}" realm="Sonatype Nexus Repository Manager" username="${username}" passwd="${password}" />
	</ivy:configure>
	
</ivysettings>


